{
  "version": 2,
  "builds": [
    {
      "src": "project/package.json",
      "use": "@vercel/next"
    }
  ],
  "functions": {
    "project/src/pages/api/**/*.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/project/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/project/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "regions": ["iad1"],
  "framework": "nextjs"
}
